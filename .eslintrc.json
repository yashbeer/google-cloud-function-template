{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "semi": ["error", "never"],
      "quotes": ["error", "single"],
      "arrow-body-style": "off"
    }
}
